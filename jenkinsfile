pipeline{
  agent any
  stages{
    stage("Git Checkout"){
      steps{
            git credentialsId: 'github', url: 'https://github.com/Devopsveeru/sonar.git'
           }
          }
     stage("Apostrophe  Build"){
       steps{
           stage('Build apostrophe') {
           steps {
               acrQuickTask(azureCredentialsId: "$env.Azuer_principal" , local:"client", imageNames: [[image: "$env.mindwaveregistry.azurecr.io/$env.liverpoolstaging-apostrophe:v$env.728" ], [image: "$env.mindwaveregistry.azurecr.io//$env.liverpoolstaging-apostrophe:latest"]], registryName: "$env.MindwaveRegistry", resourceGroupName: "$env.CommonResources", timeout: 28800)
               }
             }
            
             }
         }
      }
    }
